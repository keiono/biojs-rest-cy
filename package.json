{
  "name": "biojs-rest-cy",
  "description": "cyREST utilities for node.js",
  "version": "0.1.0",
  "homepage": "https://github.com/keiono/biojs-rest-cy",
  "author": {
    "name": "Keiichiro Ono",
    "email": "kono@ucsd.edu"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/keiono/biojs-rest-cy.git"
  },
  "bugs": {
    "url": "https://github.com/keiono/biojs-rest-cy/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/keiono/biojs-rest-cy/blob/master/LICENSE"
    }
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha --require blanket --reporter mocha-lcov-reporter > coverage/lcov.info && gulp test && gulp coveralls",
    "build": "gulp build",
    "build-browser": "gulp build-browser",
    "build-browser-min": "gulp build-browser-gzip",
    "prepublish": "gulp",
    "sniper": "biojs-sniper .",
    "watch": "gulp watch"
  },
  "dependencies": {
    "request": "^2.44.0"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "browserify": "5.x",
    "chai": "^1.9.1",
    "coveralls": "^2.11.1",
    "gulp": "^3.8.8",
    "gulp-chmod": "^1.1.1",
    "gulp-coveralls": "^0.1.3",
    "gulp-gzip": "0.0.8",
    "gulp-jshint": "^1.8.4",
    "gulp-mocha": "^1.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-rimraf": "^0.1.0",
    "gulp-streamify": "^0.0.5",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^1.0.2",
    "mkdirp": "^0.5.0",
    "mocha": "~1.20.1",
    "mocha-lcov-reporter": "0.0.1",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^1.0.6"
  },
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": "node_modules"
    },
    "travis-cov": {
      "threshold": 60
    }
  },
  "shields": {
    "test": {
      "img": "https://travis-ci.org/keiono/biojs-rest-cy.svg?branch=master",
      "href": "https://travis-ci.org/keiono/biojs-rest-cy"
    }
  },
  "keywords": [
    "cytoscape",
    "biojs"
  ]
}
